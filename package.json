{
  "name": "project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run docs:prepare && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run docs:prepare && nest build && node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",

    "docs:generate": "npx ts-node scripts/generate-openapi.ts",
    "docs:export": "mkdir -p docs && curl -s http://localhost:3005/admin/api-json -o docs/openapi.admin.json && curl -s http://localhost:3005/client/api-json -o docs/openapi.client.json && echo \"OpenAPI specs exported to docs/\"",
    "docs:build:admin": "npx @redocly/cli build-docs docs/openapi.admin.json -o docs/admin-api.html --config redocly.yaml",
    "docs:build:client": "npx @redocly/cli build-docs docs/openapi.client.json -o docs/client-api.html --config redocly.yaml",
    "docs:build": "npm run docs:build:admin && npm run docs:build:client",
    "docs:all": "npm run docs:export && npm run docs:build",
    "docs:all:open": "npm run docs:all && open docs/admin-api.html && open docs/client-api.html",
    "docs:deploy": "npm run docs:generate && npm run docs:build",
    "docs:prepare": "node scripts/prepare-redoc.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/client-ecr": "^3.654.0",
    "@aws-sdk/client-ecs": "^3.654.0",
    "@aws-sdk/client-iam": "^3.654.0",
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/client-ses": "^3.927.0",
    "@aws-sdk/client-sts": "^3.654.0",
    "@aws-sdk/s3-request-presigner": "^3.654.0",
    "@aws-sdk/util-dynamodb": "^3.654.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.1",
    "@nestjs/typeorm": "^10.0.2",
    "@scalar/nestjs-api-reference": "^1.0.13",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "country-codes-flags-phone-codes": "^1.1.1",
    "crypto": "^1.0.1",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "nestjs-cls": "^5.4.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@redocly/cli": "^2.14.3",
    "@types/aws-sdk": "^0.0.42",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.14.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.18.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
